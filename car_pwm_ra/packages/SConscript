import os
from building import *
src  = []
objs = []
cwd  = GetCurrentDir()
path =  [cwd + '/common']
LOCAL_CCFLAGS = ''
group = DefineGroup('applications', src, depend = [''], CPPPATH = path, LOCAL_CCFLAGS = LOCAL_CCFLAGS)
objs = group
list = os.listdir(cwd)
for item in list:
    if os.path.isfile(os.path.join(cwd, item, 'SConscript')):
        objs = objs + SConscript(os.path.join(item, 'SConscript'))
Return('objs')